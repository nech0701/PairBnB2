<%= form_for @reservation do |f| %>

  <div class="form-group">
    <%= f.label :check_in %>
    <%= f.date_select :start_date, :order => [:day, :month, :year]%>
  </div>

  <div class="form-group">
    <%= f.label :check_out %>
    <%= f.date_select :end_date, :order => [:day, :month, :year] %>
  </div>


  <div class="field">
 	  <%= f.hidden_field "listing_id", value: params[:id] %><br>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>

